{
  "name": "obsidian-whisper-transcribe",
  "version": "0.1.0",
  "description": "Whisper APIを使用した音声録音・文字起こしのObsidianプラグイン",
  "main": "main.js",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "plugin",
    "transcription",
    "whisper",
    "openai",
    "openai-whisper",
    "gpt-4o-transcribe",
    "voice",
    "audio",
    "speech-to-text",
    "recording"
  ],
  "author": "aftercider",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aftercider/obsidian-transcribe.git"
  },
  "bugs": {
    "url": "https://github.com/aftercider/obsidian-transcribe/issues"
  },
  "homepage": "https://github.com/aftercider/obsidian-transcribe#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.3",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "obsidian": "^1.11.4",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
